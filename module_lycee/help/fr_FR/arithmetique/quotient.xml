<refentry version="5.0-subset Scilab" xml:id="quotient" xml:lang="fr"
          xmlns="http://docbook.org/ns/docbook"
          xmlns:xlink="http://www.w3.org/1999/xlink"
          xmlns:svg="http://www.w3.org/2000/svg"
          xmlns:ns4="http://www.w3.org/1999/xhtml"
          xmlns:mml="http://www.w3.org/1998/Math/MathML"
          xmlns:db="http://docbook.org/ns/docbook">
  <info>
    <pubdate>$LastChangedDate$</pubdate>
  </info>

  <refnamediv>
    <refname>quotient</refname>

    <refpurpose>calcul du quotient de deux nombres</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <title>Séquence d'appel</title>

    <synopsis>q = quotient(m,n)</synopsis>
  </refsynopsisdiv>

  <refsection>
    <title>Paramètres</title>

    <variablelist>
      <varlistentry>
        <term>m</term>

        <listitem>
          <para>entier</para>
        </listitem>
      </varlistentry>

      <varlistentry>
        <term>n</term>

        <listitem>
          <para><literal>entier non nul</literal></para>
        </listitem>
      </varlistentry>

      <varlistentry>
        <term>q</term>

        <listitem>
          <para>entier</para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsection>

  <refsection>
    <title>Description</title>

    <para><literal>quotient(m,n)</literal> retourne le quotient de
    <literal>m</literal> par <literal>n</literal>.</para>
  </refsection>

  <refsection>
    <title>Exemples</title>

    <programlisting role="example">quotient(25,3)
quotient(-25,3)</programlisting>
  </refsection>

  <refsection>
    <title>Voir Aussi</title>

    <simplelist type="inline">
      <member><link linkend="reste">reste</link></member>
    </simplelist>
  </refsection>
</refentry>
